\node (A2) at (0,1.5) {$a_2$};

% begin second set
\node (B) at (4,8.5) {$B$};
\draw (4,7) circle (1.2cm);
\node (B1) at (4,7) {$fa_1 = fa_2$};

% begin first set
\draw (8,2) circle (1.2cm);
