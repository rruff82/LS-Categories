\node (b) at (-2,2) {$b$};
\node (p) at (-1,2) {$p$};
\node (q) at (0,2) {$q$};
\node (r) at (1,2) {$r$};
\node (s) at (2,2) {$s$};
\draw (0,2) circle [x radius=3, y radius=1.25];

\node (0) at (-1.5,-2) {$0$};
\node (1) at (1.5,-2) {$1$};
\draw (0,-2) circle [x radius=3, y radius=1.25];

