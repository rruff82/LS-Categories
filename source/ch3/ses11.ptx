                        \node[red] (G15) at (5,1) {$e$};
                        \node[red] (G16) at (4,1.3) {$\bullet$};
                        \node[red] (G17) at (5,2) {$\bullet$};
                        \node[red] (G18) at (4.5,3.2) {$\bullet$};
                        \node[red] (G19) at (3.7,2.7) {$\bullet$};
                        \node[red] (G20) at (2.9,2) {$\bullet$};
                        \node[red] (G21) at (2.7,1.4) {$\bullet$};
                        \node[red] (G22) at (1.9,1.5) {$\bullet$};6
                        \node[red] (G23) at (.8,1.2) {$\bullet$};
                        \node[red] (G24) at (.2,2.1) {$\bullet$};
                        \node[red] (G25) at (.2,2.6) {$\bullet$};
                        \node[red] (G26) at (.7,2.6) {$\bullet$};
                        \node[red] (G27) at (1.8,2.3) {$\bullet$};
                        \node[red] (G28) at (2.4,3.2) {$\bullet$};
                        \node[red] (G29) at (2.9,3.4) {$\bullet$};
        
                        \path[-{Stealth},font=\scriptsize,>=angle 90,red]
                            (G16) edge (G15)
                            (G16) edge[bend right] (G17)
                            (G17) edge[bend right] (G16)
                            (G18) edge (G17)
                            (G19) edge (G18)
                            (G19) edge[bend left] (G20)
                            (G20) edge[bend left] (G19)
                            (G21) edge (G20)
                            (G22) edge (G21)
                            (G23) edge (G22)
                            (G24) edge[bend left] (G23)
                            (G23) edge[bend left] (G24)
                            (G25) edge (G24)
                            (G26) edge[bend left] (G25)
                            (G25) edge[bend left] (G26)
                            (G27) edge (G26)
                            (G22) edge[bend left] (G27)
                            (G27) edge[bend left] (G22)
                            (G27) edge[bend left] (G28)
                            (G28) edge[bend left] (G27)
                            (G29) edge (G28)
                            (G29) edge[bend left] (G19)
                            (G19) edge[bend left] (G29)
        
                        ;
        
        
                        \path[-{Stealth},font=\scriptsize,>=angle 90,dashed]
                            (G18) edge (G5)
                            (G18) edge (G7)
                            (G19) edge (G4)
                            (G26) edge (G0)
                            (G27) edge (G10)
                            (G28) edge (G10)
                            (G28) edge (G1)
                            (G28) edge (G2) 
                            (G29) edge (G3) 
                            (G15) edge (G17)
                            (G16) edge (G8)
                            (G24) edge[bend right] (G12)
